<!-- BEGIN GLOBAL VENDORS :JS -->
<!-- jQuery first, then Popper.js, then Bootstrap JS etc.. -->
<script src="{{assets}}{{vendors}}jquery/dist/jquery.js"></script>
<script src="{{assets}}{{vendors}}popper.js/dist/umd/popper.js"></script>
<script src="{{assets}}{{vendors}}bootstrap/dist/js/bootstrap.js"></script>
<script src="{{assets}}{{vendors}}metismenu/dist/metisMenu.js"></script>
<script src="{{assets}}{{vendors}}jquery-slimscroll/jquery.slimscroll.js"></script>
<script src="{{assets}}{{vendors}}js-cookie/src/js.cookie.js"></script>
<script src="{{assets}}{{vendors}}ionicons/dist/ionicons/ionicons.js"></script>
<!-- /END GLOBAL VENDORS :JS -->

<!-- BEGIN PAGE LEVEL VENDORS :JS -->
{{#if js_cdn_vendors}}
    {{helper-cdn js_cdn_vendors}}
{{/if}}
{{#if js_vendors}}
    {{#each js_vendors}}
        <script src="{{../assets}}{{../vendors}}{{ this }}.js"></script>
    {{/each}}
{{/if}}
<!-- /END PAGE LEVEL VENDORS :JS -->

<!-- BEGIN PAGE LEVEL VENDORS EXTENDED :JS -->
{{#if js_vendors_extended}}
    {{#each js_vendors_extended}}
        <script src="{{../assets}}{{../script}}vendors-extended/{{ this }}.js"></script>
    {{/each}}
{{/if}}
<!-- /END PAGE LEVEL VENDORS EXTENDED :JS -->

<!-- BEGIN OBEUS :JS -->
<!-- Custom JavaScript code for all pages-->
<script src="{{assets}}{{script}}{{ package.name }}{{#if production}}.min{{/if}}.js"></script>
<!-- /END OBEUS :JS -->

<!-- BEGIN PAGE-SPECIFIC :JS -->
{{#if js_page_specific}}
    {{#each js_page_specific}}
        <script src="{{../assets}}{{../script}}pages/{{ this }}{{#if production}}.min{{/if}}.js"></script>
    {{/each}}
{{/if}}
<!-- /END PAGE-SPECIFIC :JS -->
