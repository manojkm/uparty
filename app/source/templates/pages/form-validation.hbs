                            <div class="col-lg-10">
                                <input type="text" class="form-control date-picker" required id="validationDatepicker" placeholder="Select date">
                                <div class="invalid-feedback">This value is required.</div>
                            </div>
                        </div>
                        <!-- /Datepicker -->

                        <!-- Checkbox -->
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Checkbox</label>
                            <div class="col-lg-10">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="validationCheckbox" required>
                                    <label class="form-check-label" for="validationCheckbox">Check this default checkbox</label>
                                    <div class="invalid-feedback">This value is required.</div>
                                </div>
                            </div>
                        </div>
                        <!-- /Checkbox -->

                        <!-- Custom checkbox -->
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Custom checkbox</label>
                            <div class="col-lg-10">
                                <div class="custom-control custom-checkbox">
