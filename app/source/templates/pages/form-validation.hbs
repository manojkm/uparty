---

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            {{#card}}
                {{{card_header 'Validation styles and supported elements'}}}
                {{#card_body_has 'example'''}}
                    <form class="was-validated">

                        <!-- Normal input text -->
                        <div class="form-group row">
                            <label for="validationNormalInputText" class="col-lg-2 col-form-label">Normal input text</label>
                            <div class="col-lg-10">
                                <input type="text" class="form-control" id="validationNormalInputText" placeholder="Normal input text" value="Mark" required>
                                <div class="invalid-feedback">This value is required.</div>
                            </div>
                        </div>
                        <!-- /Normal input text -->

                        <!-- Input group -->
                        <div class="form-group row">
                            <label for="validationInputGroup" class="col-lg-2 col-form-label">Input group</label>
                            <div class="col-lg-10">
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-user"></i></div>
                                    </div>
                                    <input type="text" class="form-control" id="validationInputGroup" placeholder="Username" required>
                                    <div class="invalid-feedback">This value is required.</div>
                                </div>
                            </div>
                        </div>
                        <!-- /Input group -->

                        <!-- Select -->
                        <div class="form-group row">
                            <label for="validationSelect" class="col-lg-2 col-form-label">Select</label>
                            <div class="col-lg-10">
                                <select class="form-control" id="validationSelect" required>
                                    <option value="">Choose One</option>
                                    <option value="1">California</option>
                                    <option value="2">Virginia</option>
                                    <option value="3">Texas</option>
                                    <option value="4">Florida</option>
                                </select>
                                <div class="invalid-feedback">This value is required.</div>
                            </div>
                        </div>
                        <!-- /Select -->

                        <!-- Multiple select -->
                        <div class="form-group row">
                            <label for="validationMultiSelect" class="col-lg-2 col-form-label">Multiple select</label>
                            <div class="col-lg-10">
                                <select multiple class="form-control" required id="validationMultiSelect">
