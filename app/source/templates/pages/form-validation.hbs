                                    <label class="custom-control-label" for="validationCustomCheckbox">Check this custom checkbox</label>
                                    <div class="invalid-feedback">This value is required.</div>
                                </div>
                            </div>
                        </div>
                        <!-- /Custom checkbox -->

                        <!-- Switch -->
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Switch</label>
                            <div class="col-lg-10">
                                <label class="custom-control switch switch-success">
                                    <input class="custom-control-input" required type="checkbox">
                                    <span class="custom-control-indicator"></span>
                                    <span class="custom-control-description">Check this Switch</span>
                                </label>
                            </div>
                        </div>
                        <!-- /Switch -->

                        <!-- Radio -->
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Radio</label>
                            <div class="col-lg-10">
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="exampleRadio" id="validationRadio" required>
                                    <label class="form-check-label" for="validationRadio">Check this default Radio</label>
                                    <div class="invalid-feedback">This value is required.</div>
                                </div>
                            </div>
                        </div>
                        <!-- /Radio -->

                        <!-- Custom Radio -->
