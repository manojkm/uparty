                        {{#list-group-item 'd-flex align-items-center'}}<i class="icon-heart mr-2"></i>{{list-group.list_item.2.text}}{{/list-group-item}}
                        {{#list-group-item 'd-flex align-items-center'}}<i class="icon-bubbles mr-2"></i>{{list-group.list_item.3.text}}{{/list-group-item}}
                    {{/list-group}}
                    <!-- /Left icons -->
                {{/card_body}}
            {{/card}}
        </div>
        <div class="col-md-6">
            {{#card}}
                {{{card_header 'Right icons'}}}
                {{#card_body}}
                    <!-- Right icons -->
                    {{#list-group ''}}
                        {{#list-group-item 'd-flex justify-content-between align-items-center'}}{{list-group.list_item.1.text}}<i class="icon-like"></i>{{/list-group-item}}
                        {{#list-group-item 'd-flex justify-content-between align-items-center'}}{{list-group.list_item.2.text}}<i class="icon-heart"></i>{{/list-group-item}}
                        {{#list-group-item 'd-flex justify-content-between align-items-center'}}{{list-group.list_item.3.text}}<i class="icon-bubbles"></i>{{/list-group-item}}
                    {{/list-group}}
                    <!-- /Right icons -->
                {{/card_body}}
            {{/card}}
        </div>
    </div>

    <h4 class="mt-4">List group with badges</h4>
    <h6 class="mb-3">Add badges to any list group item to show unread counts, activity, and more with the help of some utilities.</h6>
    <div class="row">
        <div class="col-md-6">
            {{#card}}
                {{{card_header 'Left badges'}}}
                {{#card_body}}
