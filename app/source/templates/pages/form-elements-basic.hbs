---
title: "Basic Form Elements"
lead: Elements for creating great user experiences.
forms-ui: true

js_vendors:
- autosize/dist/autosize
---

<!--TODO : XL sizing on input, select etc.. -->
<div class="container-fluid">
    <div class="row">
        <div class="col-xl-10 offset-xl-1">
            {{#card}}
                {{{card_header 'Input Text'}}}
                {{#card_body_has 'example'''}}
                    <form>
                        <!-- Basic input -->
                        <div class="form-group row">
                            <label for="inputNormal" class="col-lg-2 col-form-label text-lg-right">Basic input</label>
                            <div class="col-lg-10">
                                <input type="text" class="form-control" id="inputNormal" placeholder="Normal">
                            </div>
                        </div>
                        <!-- /Basic input -->
                        <!-- Iconic -->
                        <div class="form-group row">
                            <label for="inputIconic" class="col-lg-2 col-form-label text-lg-right">Iconic</label>
                            <div class="col-lg-10">
                                <div class="input-group mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-user"></i></div>
                                    </div>
                                    <input type="text" class="form-control" id="inputIconic" placeholder="Username">
                                </div>

                                <div class="input-group">
                                    <input type="email" class="form-control" id="inputIconic2" placeholder="Email">
                                    <div class="input-group-append">
                                        <div class="input-group-text"><i class="icon-envelope"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Iconic -->

                        <!-- Seamless iconic -->
                        <div class="form-group row">
                            <label for="inputNormal" class="col-lg-2 col-form-label text-lg-right">Seamless iconic</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-user"></i></div>
                                    </div>
                                    <input type="text" class="form-control" id="inputIconic4" placeholder="Username">
                                </div>
                                <div class="input-group with-addon-icon-right">
                                    <input type="email" class="form-control" id="inputIconic3" placeholder="Email">
                                    <div class="input-group-append">
                                        <div class="input-group-text"><i class="icon-envelope"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Seamless iconic -->

                        <!-- Input text with help text -->
                        <div class="form-group row">
                            <label for="inputPassword5" class="col-lg-2 col-form-label text-lg-right">Help text</label>
                            <div class="col-lg-10">
                                <input type="password" class="form-control" id="inputPassword5" placeholder="Password">
                                <small id="passwordHelpBlock" class="form-text text-muted  mb-2">
                                    Your password must be 8-20 characters long, contain letters and numbers, and must
                                    not contain spaces, special characters, or emoji.
                                </small>
                                <div class="form-row align-items-center">
                                    <div class="col-7">
                                        <input type="password" id="inputPassword6" class="form-control" placeholder="Password" aria-describedby="passwordHelpInline">
                                    </div>
                                    <div class="col">
                                        <small id="passwordHelpInline" class="text-muted">Must be 8-20 characters long.</small>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Input text with help text -->

                        <!-- Required input -->
                        <div class="form-group row">
                            <label for="inputRequired" class="col-lg-2 col-form-label text-lg-right required-field">Required</label>
                            <div class="col-lg-10">
                                <input type="text" class="form-control mb-2" id="inputRequired" placeholder="Required" required>
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" id="agree">
                                    <label class="custom-control-label" for="agree"><span class="required-field">I agree to the terms and conditions</span></label>
                                </div>
                            </div>
                        </div>
                        <!-- /Required input -->

                        <!-- Rounded input -->
                        <div class="form-group row">
                            <label for="inputRound" class="col-lg-2 col-form-label text-lg-right">Rounded</label>
                            <div class="col-lg-10">
                                <input type="text" class="form-control input-round" id="inputRound" placeholder="Rounded input">
                            </div>
                        </div>
                        <!-- /Rounded input -->

                        <!-- Popover input -->
                        <div class="form-group row">
                            <label for="inputPopover" class="col-lg-2 col-form-label text-lg-right">Popover input</label>
                            <div class="col-lg-10">
                                <input type="text" id="inputPopover" class="form-control"
                                       placeholder="Input focus trigger popover" data-html="true" data-container="body"
                                       data-toggle="popover" data-trigger="focus" data-placement="right"
                                       data-content="And here's some amazing content. <b>It's very engaging</b>. Right?"
                                       data-original-title="Popover">
                            </div>
                        </div>
                        <!-- /Popover input -->

                        <!-- Disabled input -->
                        <div class="form-group row">
                            <label for="disabledTextInput" class="col-lg-2 col-form-label text-lg-right">Disabled</label>
                            <div class="col-lg-10">
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="Disabled input" disabled>
                            </div>
                        </div>
                        <!-- /Disabled input -->

                        <!-- Disabled iconic input -->
                        <div class="form-group row">
                            <label for="inputDisabledIconic" class="col-lg-2 col-form-label text-lg-right">Disabled
                                Iconic</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-right">
                                    <input type="text" class="form-control" id="inputDisabledIconic"
                                           placeholder="Disabled input" disabled>
                                    <div class="input-group-append">
                                        <div class="input-group-text disabled"><i class="icon-envelope"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Disabled iconic input -->

                        <!-- Readonly input -->
                        <div class="form-group row">
                            <label for="inputReadonly" class="col-lg-2 col-form-label text-lg-right">Readonly</label>
                            <div class="col-lg-10">
                                <input class="form-control" type="text" id="inputReadonly" placeholder="You can't update me :P" readonly>
                            </div>
                        </div>
                        <!-- /Readonly input -->

                        <!-- Readonly iconic input -->
                        <div class="form-group row">
                            <label for="inputReadonlyIconic" class="col-lg-2 col-form-label text-lg-right">Readonly Iconic</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-right">
                                    <input type="text" class="form-control" id="inputReadonlyIconic" placeholder="You can't update me :P" readonly>
                                    <div class="input-group-append">
                                        <div class="input-group-text readonly"><i class="icon-book-open"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Readonly iconic input -->

                        <!-- Readonly plain text -->
                        <div class="form-group row">
                            <label for="inputReadonlyPlain-Text" class="col-lg-2 col-form-label text-lg-right">Readonly plain text</label>
                            <div class="col-lg-10">
                                <input type="text" readonly class="form-control-plaintext" id="inputReadonlyPlain-Text" value="email@example.com">
                            </div>
                        </div>
                        <!-- /Readonly plain text -->


                        <!-- Sizing -->
                        <div class="form-group row">
                            <label for="InputSizing" class="col-lg-2 col-form-label text-lg-right">Sizing </label>
                            <div class="col-lg-10">
                                <input class="form-control form-control-lg mb-2" type="text" placeholder=".form-control-lg">
                                <input class="form-control mb-2" type="text" placeholder="Default input">
                                <input class="form-control form-control-sm mb-2" type="text" placeholder=".form-control-sm">
                            </div>
                        </div>
                        <!-- /Sizing -->

                        <!-- Rounded sizing -->
                        <div class="form-group row">
                            <label for="InputRoundedSizing" class="col-lg-2 col-form-label text-lg-right">Rounded sizing </label>
                            <div class="col-lg-10">
                                <input class="form-control input-round form-control-lg mb-2" type="text" placeholder=".form-control-lg">
                                <input class="form-control input-round mb-2" type="text" placeholder="Default input">
                                <input class="form-control input-round form-control-sm mb-2" type="text" placeholder=".form-control-sm">
                            </div>
                        </div>
                        <!-- /Rounded sizing -->

                        <!-- Column sizing -->
                        <div class="form-group row">
                            <label for="InputSizing" class="col-lg-2 col-form-label text-lg-right">Column sizing</label>
                            <div class="col-lg-10">

                                <div class="form-row mb-2">
                                    <div class="col-6">
                                        <input type="text" class="form-control" placeholder="6 Wide">
                                    </div>
                                    <div class="col-6">
                                        <input type="text" class="form-control" placeholder="6 Wide">
                                    </div>
                                </div>

                                <div class="form-row mb-2">
                                    <div class="col-4">
                                        <input type="text" class="form-control" placeholder="4 Wide">
                                    </div>
                                    <div class="col-4">
                                        <input type="text" class="form-control" placeholder="4 Wide">
                                    </div>
                                    <div class="col-4">
                                        <input type="text" class="form-control" placeholder="4 Wide">
                                    </div>
                                </div>

                                <div class="form-row mb-2">
                                    <div class="col-2">
                                        <input type="text" class="form-control" placeholder="2 Wide">
                                    </div>
                                    <div class="col-8">
                                        <input type="text" class="form-control" placeholder="8 Wide">
                                    </div>
                                    <div class="col-2">
                                        <input type="text" class="form-control" placeholder="2 Wide">
                                    </div>
                                </div>

                                <div class="form-row mb-2">
                                    <div class="col-6">
                                        <input type="text" class="form-control" placeholder="6 Wide">
                                    </div>
                                    <div class="col-3">
                                        <input type="text" class="form-control" placeholder="3 Wide">
                                    </div>
                                    <div class="col-3">
                                        <input type="text" class="form-control" placeholder="3 Wide">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /Column sizing -->
                    </form>
                {{/card_body_has}}
            {{/card}}
        </div>
    </div>
    <div class="row">
        <div class="col-xl-10 offset-xl-1">
            {{#card}}
                {{{card_header 'Label sizing'}}}
                {{#card_body_has 'example'''}}
                    <form>
                        <!-- Label sizing -->
                        <div class="form-group row">
                            <label for="colFormLabelSm" class="col-sm-2 col-form-label col-form-label-sm text-lg-right">Email</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control form-control-sm" id="colFormLabelSm" placeholder="col-form-label-sm">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="colFormLabel" class="col-sm-2 col-form-label text-lg-right">Email</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="colFormLabel" placeholder="col-form-label">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="colFormLabelLg" class="col-sm-2 col-form-label col-form-label-lg text-lg-right">Email</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control form-control-lg" id="colFormLabelLg" placeholder="col-form-label-lg">
                            </div>
                        </div>
                        <!-- /Label sizing -->
                    </form>
                {{/card_body_has}}
            {{/card}}
        </div>
    </div>

    <div class="row">
        <div class="col-xl-10 offset-xl-1">
            {{#card}}
                {{{card_header 'Input text types'}}}
                {{#card_body_has 'example'''}}
                    <form>
                        <!-- Email type -->
                        <div class="form-group row">
                            <label for="inputEmail" class="col-lg-2 col-form-label text-lg-right">Email</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-envelope"></i></div>
                                    </div>
                                    <input class="form-control" type="email" value="bootstrap@example.com" placeholder="Email" id="inputEmail">
                                </div>
                            </div>
                        </div>
                        <!-- /Email type -->

                        <!-- URL type -->
                        <div class="form-group row">
                            <label for="inputURL" class="col-lg-2 col-form-label text-lg-right">URL</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-link"></i></div>
                                    </div>
                                    <input class="form-control" type="url" value="https://getbootstrap.com" placeholder="URL" id="inputURL">
                                </div>
                            </div>
                        </div>
                        <!-- /URL type -->

                        <!-- Password type -->
                        <div class="form-group row">
                            <label for="inputPassword" class="col-lg-2 col-form-label text-lg-right">Password</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text"><i class="icon-lock"></i></div>
                                    </div>
                                    <input class="form-control" type="password" value="password" id="inputPassword">
                                </div>
                            </div>
                        </div>
                        <!-- /Password type -->

                        <!-- Number type -->
                        <div class="form-group row">
                            <label for="inputNumber" class="col-lg-2 col-form-label text-lg-right">Number</label>
                            <div class="col-sm-10">
                                <input class="form-control" type="number" value="34" id="inputNumber">
                            </div>
                        </div>
                        <!-- /Number type -->

                        <!-- Telephone type -->
                        <div class="form-group row">
                            <label for="inputTelephone" class="col-lg-2 col-form-label text-lg-right">Telephone</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-phone"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="tel" value="1-(555)-555-5555" id="inputTelephone">
                                </div>
                            </div>
                        </div>
                        <!-- /Telephone type -->

                        <!-- Date and time type -->
                        <div class="form-group row">
                            <label for="inputDateTime" class="col-lg-2 col-form-label text-lg-right">Date and time</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="datetime-local" id="inputDateTime">
                                </div>
                            </div>
                        </div>
                        <!-- /Date and time type -->

                        <!-- Date type -->
                        <div class="form-group row">
                            <label for="inputDate" class="col-lg-2 col-form-label text-lg-right">Date</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="date" id="inputDate">
                                </div>
                            </div>
                        </div>
                        <!-- /Date type -->

                        <!-- Month type -->
                        <div class="form-group row">
                            <label for="inputMonth" class="col-lg-2 col-form-label text-lg-right">Month</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="month" id="inputMonth">
                                </div>
                            </div>
                        </div>
                        <!-- /Month type -->

                        <!-- Week type -->
                        <div class="form-group row">
                            <label for="inputWeek" class="col-lg-2 col-form-label text-lg-right">Week</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-calendar"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="week" id="inputWeek">
                                </div>
                            </div>
                        </div>
                        <!-- /Week type -->

                        <!-- Time type -->
                        <div class="form-group row">
                            <label for="inputTime" class="col-lg-2 col-form-label text-lg-right">Time</label>
                            <div class="col-lg-10">
                                <div class="input-group with-addon-icon-left">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <i class="icon-clock"></i>
                                        </div>
                                    </div>
                                    <input class="form-control" type="time" id="inputTime">
                                </div>
                            </div>
                        </div>
                        <!-- /Time type -->

                        <!-- Color type -->
                        <div class="form-group row">
                            <label for="inputColor" class="col-lg-2 col-form-label text-lg-right">Color</label>
                            <div class="col-lg-10">
                                <input class="form-control" type="color" value="#555555" id="inputColor" style="min-height: 40px;">
                            </div>
                        </div>
                        <!-- Color type -->

                        <!-- Range type -->
                        <div class="form-group row">
                            <label for="inputRange" class="col-lg-2 col-form-label text-lg-right">Range</label>
                            <div class="col-lg-10">
                                <input type="range" class="form-control" id="inputRange" value="50">
                            </div>
                        </div>
                        <!-- /Range type -->
                    </form>
                {{/card_body_has}}
            {{/card}}
        </div>
    </div>

    <div class="row">
        <div class="col-xl-10 offset-xl-1">
            {{#card}}
                {{{card_header 'Textarea'}}}
                {{#card_body_has 'example'''}}
                    <form>
                        <!-- Basic textarea -->
                        <div class="form-group">
                            <label for="textareaNormal" class="col-form-label">Basic textarea</label>
                            <textarea class="form-control" id="textareaNormal" rows="3" placeholder="Let us type some lorem ipsum...."></textarea>
                        </div>
                        <!-- /Basic textarea -->

                        <!-- Iconic textarea -->
                        <div class="form-group">
                            <label for="textareaNormalIconic" class="col-form-label">Iconic textarea</label>
                            <div class="input-group with-addon-icon-left">
                                <div class="input-group-prepend">
                                    <div class="input-group-text align-items-start"><i class="icon-note"></i></div>
                                </div>
                                <textarea class="form-control" id="textareaNormalIconic" rows="3" placeholder="Let us type some lorem ipsum...."></textarea>
                            </div>
                        </div>
                        <!-- /Iconic textarea -->

                        <!-- Height Auto Resize -->
                        <div class="form-group">
                            <label for="textareaHeightAutosize" class="col-form-label">Height Auto Resize</label>
                            <textarea class="form-control" id="textareaHeightAutosize" data-plugin="autosize"
                                      placeholder="Start pressing Enter to see growing..."></textarea>
                        </div>
                        <!-- /Height Auto Resize -->

                        <!-- No resize -->
                        <div class="form-group">
                            <label for="textareaNoResize" class="col-form-label">No resize</label>
                            <textarea class="form-control textarea-no-resize" id="textareaNoResize" rows="3" placeholder="Let us type some lorem ipsum...."></textarea>
                        </div>
                        <!-- /No resize -->

                        <!-- Character counter -->
                        <div class="form-group">
                            <label for="textareaCounter" class="col-form-label">Character counter</label>
                            <textarea rows="3" class="form-control" data-toggle="counter" id="textareaCounter" placeholder="Let us type some lorem ipsum...."></textarea>
                            <small class="character-remaining">125 characters left</small>
                        </div>
                        <!-- /Character counter -->

                        <!-- Disabled -->
                        <div class="form-group">
                            <label for="textareaDisabled" class="col-form-label">Disabled</label>
                            <textarea class="form-control" id="textareaDisabled" placeholder="Disabled textarea" disabled></textarea>
                        </div>
                        <!-- /Disabled -->

                    </form>
                {{/card_body_has}}
            {{/card}}
        </div>
    </div>

    <div class="row">
        <div class="col-xl-10 offset-xl-1">
            {{#card}}
                {{{card_header 'Select menu'}}}
                {{#card_body_has 'example'''}}
                    <form>
                        <!-- Basic select -->
                        <div class="form-group row">
                            <label for="selectNormal" class="col-lg-2 col-form-label text-lg-right">Basic select</label>
                            <div class="col-lg-10">
                                <select class="form-control" id="selectNormal">
                                    <option value="">Choose One</option>
                                    <option value="1">California</option>
                                    <option value="2">Virginia</option>
                                    <option value="3">Texas</option>
                                    <option value="4">Florida</option>
                                </select>
                            </div>
                        </div>
                        <!-- /Basic select -->

                        <!-- Multiple select -->
                        <div class="form-group row">
                            <label for="selectMultiple" class="col-lg-2 col-form-label text-lg-right">Multiple select</label>
                            <div class="col-lg-10">
                                <select multiple class="form-control" id="selectMultiple">
                                    <optgroup label="Picnic">
                                        <option>Mustard</option>
                                        <option>Ketchup</option>
                                        <option>Relish</option>
                                    </optgroup>
                                    <optgroup label="Camping">
                                        <option>Tent</option>
                                        <option>Flashlight</option>
                                        <option>Toilet Paper</option>
                                    </optgroup>
                                </select>

                            </div>
                        </div>
                        <!-- /Multiple select -->

                        <!-- Disabled -->
                        <div class="form-group row">
                            <label for="selectDisabled" class="col-lg-2 col-form-label text-lg-right">Disabled</label>
                            <div class="col-lg-10">
                                <select class="form-control" id="selectDisabled" disabled>
                                    <option value="">Choose One</option>
                                    <option value="1">California</option>
                                    <option value="2">Virginia</option>
                                    <option value="3">Texas</option>
                                    <option value="4">Florida</option>
                                </select>
                            </div>
                        </div>
                        <!-- /Disabled -->

                        <!-- Custom select -->
                        <div class="form-group row">
                            <label for="selectCustom" class="col-lg-2 col-form-label text-lg-right">Custom select</label>
                            <div class="col-lg-10">
                                <select class="custom-select" id="selectCustom">
                                    <optgroup label="Picnic">
                                        <option>Mustard</option>
                                        <option>Ketchup</option>
                                        <option>Relish</option>
                                    </optgroup>
                                    <optgroup label="Camping">
                                        <option>Tent</option>
                                        <option>Flashlight</option>
                                        <option>Toilet Paper</option>
                                    </optgroup>
                                </select>
                            </div>
                        </div>
                        <!-- /Custom select -->

                        <!-- Custom select disabled -->
                        <div class="form-group row">
                            <label for="selectCustomDisabled" class="col-lg-2 col-form-label text-lg-right">Custom select disabled</label>
                            <div class="col-lg-10">
                                <select class="custom-select" id="selectCustomDisabled" disabled>
                                    <optgroup label="Picnic">
                                        <option>Mustard</option>
                                        <option>Ketchup</option>
                                        <option>Relish</option>
                                    </optgroup>
                                    <optgroup label="Camping">
                                        <option>Tent</option>
                                        <option>Flashlight</option>
                                        <option>Toilet Paper</option>
                                    </optgroup>
                                </select>

                            </div>
                        </div>
                        <!-- /Custom select disabled -->

                        <!-- Sizing - small -->
                        <div class="form-group row">
                            <label for="selectSizingSmall" class="col-lg-2 col-form-label col-form-label-sm text-lg-right">Sizing - small</label>
                            <div class="col-lg-10">
